<style>
table, th, td {
  border:1px solid black;
}
</style>
        <table class="table">
            <thead>
                <tr>
                    <th>Student ID</th>
                    <th>Name</th>
                    <th>Program</th>
                    <th>GPA</th>
                </tr>
            </thead>
            <tbody>
                {{#each students}}
                <tr>
                    <td>{{studId}}</td>
                    <td>{{name}}</td>
                    <td>{{program}}</td>
                    <td>{{gpa}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    